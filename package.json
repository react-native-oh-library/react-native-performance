{
  "name": "@react-native-oh-tpl/react-native-performance",
  "version": "5.1.2-0.0.1",
  "description": "Measure React Native performance",
  "homepage": "https://github.com/oblador/react-native-performance",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-native-oh-library/react-native-performance.git",
    "directory": "packages/react-native-performance"
  },
  "harmony": {
    "alias": "react-native-performance",
    "codegenConfig": {
      "specPaths": [
        "./src"
      ]
    }
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "typings": "lib/typescript/index.d.ts",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "harmony",
    "ios",
    "react-native-performance.*",
    "!.DS_Store",
    "!android/build",
    "!ios/build"
  ],
  "scripts": {
    "blob": "bob build",
    "test": "jest",
    "prepare": "npm run blob"
  },
  "keywords": [
    "react-native",
    "performance",
    "perf",
    "benchmark",
    "harmony"
  ],
  "author": "Joel Arvidsson",
  "license": "MIT",
  "peerDependencies": {
    "react-native": "*"
  },
  "dependencies": {
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-typescript": "^7.20.0",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.3.3",
    "babel-jest": "^29.2.1",
    "jest": "^29.2.1",
    "react-native-builder-bob": "^0.21.2",
    "typescript": "^4.8.3"
  },
  "codegenConfig": {
    "name": "RNPerformanceSpec",
    "type": "modules",
    "jsSrcsDir": "./src",
    "android": {
      "javaPackageName": "com.oblador.performance"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "gitHead": "c0d8d1a41311bfde7eafd4d9ff502b463732e510"
}
